{
    "version":2,
    "builds" :[
        {
            "src":"chatapp/wsgi.py",
            "use":"@vercel/python",
            "config":{"maxLambdaSize": "10mb", "runtime":"python3.12"}
        },
        {
            "src":"build_files.sh",
            "use":"@vercel/static-build",
            "config":{
                "distDir":"staticfiles"
            }
        }
    ],
    "routes":[
        {
            "src":"/static/(.*)",
            "dest":"/static/$1"
        },
        {
            "src":"/(.*)",
            "dest":"chatapp.wsgi.application"
        }
    ],
    "env": {
        "DJANGO_SETTINGS_MODULE": "chatapp.settings"
    }
}